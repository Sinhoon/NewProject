<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.study.web.mapper.MemberMapper">
    <resultMap type="Member" id="MemberResultMap">
        <result property="name" column="uNum" />
        <result property="email" column="uEmail" />
        <result property="phone" column="uPhone" />
    </resultMap>
    <!-- id의 경우에는 Mapper.java에 동일하게 사용해야된다. -->
    <select id="getMembers" resultMap="MemberResultMap">
        SELECT * FROM user
    </select>

</mapper>
